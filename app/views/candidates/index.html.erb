<div class="container">
  <div class="row">
    <h3>Candidates</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @candidates.each do |candidate| %>
            <tr>
              <%= render candidate %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
